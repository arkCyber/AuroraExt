{
  "name": "auroraext",
  "displayName": "AuroraExt: Enhanced Web UI for AI Models",
  "version": "1.0.9",
  "description": "Enhanced browser extension with AI models and blockchain features for web browsing assistance.",
  "author": "arkSong <arksong2018@gmail.com>",
  "homepage": "https://github.com/arkCyber/AuroraExt",
  "repository": {
    "type": "git",
    "url": "https://github.com/arkCyber/AuroraExt.git"
  },
  "bugs": {
    "url": "https://github.com/arkCyber/AuroraExt/issues"
  },
  "type": "module",
  "scripts": {
    "dev": "cross-env TARGET=chrome wxt",
    "dev:firefox": "cross-env TARGET=firefox wxt -b firefox",
    "dev:edge": "cross-env TARGET=chrome wxt -b edge",
    "build": "cross-env TARGET=chrome wxt build && node copy-wasm.js",
    "build:firefox": "cross-env TARGET=firefox wxt build -b firefox",
    "build:edge": "cross-env TARGET=chrome wxt build -b edge",
    "zip": "cross-env TARGET=chrome wxt zip",
    "zip:firefox": "cross-env TARGET=firefox wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "postinstall": "wxt prepare",
    "test": "vitest run",
    "test:wallet": "ts-node --esm src/utils/test-wallet.ts",
    "lint": "eslint src",
    "format": "prettier --write src/**/*.{ts,tsx,css,scss,json,md}",
    "test:watch": "vitest"
  },
